{
  "name": "svg-to-png-loader",
  "version": "2.0.2",
  "description": "webpack loader which converts SVG files to PNG files",
  "license": "MIT",
  "repository": "github1/svg-to-png-loader",
  "bugs": {
    "url": "https://github.com/github1/svg-to-png-loader/issues"
  },
  "homepage": "https://github.com/github1/svg-to-png-loader#readme",
  "author": "John Collier <john.collier4@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "loader-utils": "^2.0.0"
  },
  "peerDependencies": {
    "webpack": ">=2"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "ava": "^3.15.0",
    "babel-plugin-istanbul": "^6.0.0",
    "convert-svg-to-png": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "memfs": "^3.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "clean": "rimraf lib/",
    "lint": "eslint src",
    "test": "npm run lint && cross-env BABEL_ENV=test npm run build && nyc ava",
    "build": "babel src/ --out-dir lib/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webpack",
    "loader",
    "svg",
    "png"
  ],
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "json"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "babel": true
  }
}
